{
    "name": "mcire/l5-auto-swagger",
    "description": "This package helps to install Swagger automatically.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mcire\\L5AutoSwagger\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "cd /../../../.. && composer require darkaonline/l5-swagger",
            "cd /../../../.. && php artisan vendor:publish --provider=\"L5Swagger\\L5SwaggerServiceProvider\"",
            "cd cd /../../../.. && php artisan l5-swagger:generate"
        ]
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "darkaonline/l5-swagger": "^8.0"
    },
    "version": "1.0.4"
}

