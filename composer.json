{
    "name": "mcire/l5-auto-swagger",
    "description": "This package helps to install Swagger automatically with one command",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mcire\\L5AutoSwagger\\": "vendor/mcire/l5-auto-swagger/src/"
        }
    },
    
    "files": [
        "src/Commands/InstallSwagger.php"
    ],
    "authors": [
        {
            "name": "babyd12"
        }
    ],
    "minimum-stability": "stable", 
    "require": {
        "php": ">=5.3.0",
        "darkaonline/l5-swagger": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "composer require darkaonline/l5-swagger",
            "php artisan vendor:publish --provider=\"L5Swagger\\L5SwaggerServiceProvider\"",
            "php artisan l5-swagger:generate",

            "__DIR__.'/../vendor/bin/composer' require darkaonline/l5-swagger --working-dir=../../../../",
            "__DIR__.'/../php' artisan vendor:publish --provider=\"L5Swagger\\L5SwaggerServiceProvider\" --working-dir=../../../../",
            "__DIR__.'/../php' artisan l5-swagger:generate --working-dir=../../../../",
            "php AutoSwagger.sh --working-dir=../../../../"
        ],
        "post-update-cmd": [
            "composer require darkaonline/l5-swagger",
            "php artisan vendor:publish --provider=\"L5Swagger\\L5SwaggerServiceProvider\"",
            "php artisan l5-swagger:generate",

            "__DIR__.'/../vendor/bin/composer' require darkaonline/l5-swagger",
            "__DIR__.'/../php' artisan vendor:publish --provider=\"L5Swagger\\L5SwaggerServiceProvider\"",
            "__DIR__.'/../php' artisan l5-swagger:generate"
        ]
    },
    "version": "1.0.2"
}
