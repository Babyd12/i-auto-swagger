{
    "name": "mcire/auto-swagger",
    "description": "Librairie pour installer Swagger automatiquement",
    "type": "library",
    "scripts": {
        "post-install-cmd": [
            "echo 'Execution du code PHP' > __ROOT__/auto-swagger.txt",
            "echo 'Execution du code PHP' > __ROOT__/AutoSwagger.sh",
            "./install-swagger.sh"
        ]
    },
    "autoload": {
        "psr-4": {
            "Mcire\\AutoSwagger\\": "src/"
        }
    }
}
