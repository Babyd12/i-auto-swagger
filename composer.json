{
    "name": "micre/swagger-installer",
    "description": "A mini library to automate Swagger installation in Laravel projects.",
    "require": {
        "php": ">=6.2.0",
        "darkaonline/l5-swagger": "^6.0",
        "tymon/jwt-auth": "^1.0",
        "l5swagger/l5swagger": "^8.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Micre\\SwaggerInstaller\\Installer::installSwagger"
        ]
    },
    "autoload": {
        "psr-4": {
            "Micre\\SwaggerInstaller\\": "src/"
        }
    }
}
